@model IEnumerable< MVCDemo.Models.EmpModel>

<a asp-action = "Create">Create</a>
<table class="table table-striped">
    <thead>
    <th>Id</th>
    <th>First Name</th>
    <th>Last Name</th>
    <th>Email</th>
    <th>DOB</th>
    <th>Contact</th>
    <th>Designation</th>
    <th>Department</th>
    <th>City</th>
    <th>Action</th>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>@item.Id</td>
                <td>@item.FirstName</td>
                <td>@item.LastName</td>
                <td>@item.Email</td>
                <td>@item.DOB</td>
                <td>@item.ContactNo</td>
                <td>@item.Designation</td>
                <td>@item.Department</td>
                <td>@item.City</td>
                <td>@Html.ActionLink("Edit","Edit",new{id = item.Id})|@Html.ActionLink("Delete","Delete",new{id = item.Id})</td>
            </tr>
        }
    </tbody>
</table>
